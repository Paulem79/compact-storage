{
  "schemaVersion": 1,
  "id": "compact_storage",
  "version": "${version}",

  "name": "Compact Storage",
  "description": "CompactStorage adds various forms of storage to Minecraft, all of which allow you to store lots of stuff!",
  "authors": [
    "Tabitha Strong"
  ],
  "contact": {
    "homepage": "https://tabithastrong.com/minecraft/",
    "sources": "https://github.com/tabithastrong/compact_storage"
  },

  "license": "CC0-1.0",
  "icon": "assets/compact_storage/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "com.tabithastrong.compactstorage.CompactStorage"
    ],
    "client": [
      "com.tabithastrong.compactstorage.CompactStorageClient"
    ]
  },
  "mixins": [
  ],
  "accessWidener": "compact_storage.accesswidener",

  "depends": {
    "fabricloader": ">=0.13.3",
    "fabric": "*",
    "minecraft": "~1.18.2",
    "java": ">=17"
